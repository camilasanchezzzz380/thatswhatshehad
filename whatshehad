<script type="text/javascript">
  
  jQuery(document).ready(function(){
    var ls_ajaxurl = 'https://thatswhatshehad.com/wp-admin/admin-ajax.php';

    //Added active class to popup after 3 min so popupup will be disapl after 3 min
    setTimeout(function(){
      jQuery(".ls_popupmain#newsletterfrm").addClass("active");
    }, 180000); //3 min


    jQuery(".ls_popupmain .ls_popupclose").click(function(){
      var current_popup = jQuery(this).parents(".ls_popupmain").attr("id");

      jQuery(this).parents(".ls_popupmain").removeClass("active");
      if(current_popup == 'newsletterfrm'){
        jQuery.ajax({
            url:ls_ajaxurl,
            data:{
              action: 'ls_setcookie_newsletterfrm',
            },
            method: "POST",
            dataType: "JSON",
            success: function( response ){
              //console.log(response.status);
          }
            
          }); 
      }
      
      
    });
  });

